# Build the module

MODULE_SUFFIX ?= Custom_module

ifeq (${BUILD64},1)
OASM = o.asm64
else
OASM = o.asm
endif

COMPONENT  = Hourglass
CDEFINES   = -DMODULE_SUFFIX=${MODULE_SUFFIX}
INCLUDES   =
LIBS       =
OBJS       = \
             ${OASM} \
             o.modhead \
             o.module

include CModule

${OZDIR}.module: h.modhead
${OZDIR}.modhead: VersionNum
