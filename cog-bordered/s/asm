          DCD     &83955557, &bffbfafe
          DCD     &83955557, &aabbaaee
          DCD     &83955557, &aebbaafe
          DCD     &83ffffff, &aaaaaaaa
          ; 85%
          DCD     &00000000, &00000000
          DCD     &83ffffff, &aaaaaaaa
